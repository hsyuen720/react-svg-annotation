{
  "name": "react-svg-annotation",
  "description": "SVG Annotation Component",
  "keywords": [
    "svg",
    "annotation",
    "svg-drawing"
  ],
  "version": "0.0.14",
  "author": "Benny Yuen",
  "repository": {
    "type": "git",
    "url": "https://github.com/hsyuen720/react-svg-annotation"
  },
  "license": "MIT",
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/react-svg-annotation.cjs",
    "default": "./dist/react-svg-annotation.modern.js"
  },
  "main": "./dist/react-svg-annotation.cjs",
  "module": "./dist/react-svg-annotation.module.js",
  "unpkg": "./dist/react-svg-annotation.umd.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "microbundle build --globals react=React,react-dom=ReactDOM --jsx React.createElement --output dist",
    "dev": "microbundle watch",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "serve-storybook": "serve storybook-static",
    "lint": "eslint --ext=.ts,.tsx ./src --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uniqid": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "babel-loader": "^8.3.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "microbundle": "^0.15.1",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "immer": "^9.0.16",
    "uniqid": "^5.4.0"
  }
}
